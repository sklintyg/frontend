{
  "name": "@frontend/components",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs"
      },
      "import": {
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch",
    "test": "vitest",
