  "dependencies": {
    "@floating-ui/react": "^0.25.1",
    "@frontend/ids-react-ts": "workspace:*",
    "@inera/ids-core": "^4.10.0",
    "@inera/ids-design": "^4.10.0",
    "react": "^18.2.0",
    "react-focus-lock": "^2.9.6",
