{
  "name": "@frontend/common",
  "version": "0.1.0",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "./dist/common.umd.js",
  "module": "./dist/common.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/common.es.js",
      "require": "./dist/common.umd.js"
    }
  },
  "scripts": {
    "dev": "vite build -w -m development",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "typescript": "tsc --noEmit",
    "index-list": "ctix create -pw ./tsconfig.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "jest --bail --findRelatedTests"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "date-fns": "^2.16.1",
    "dompurify": "^2.3.6",
    "faker": "5.5.3",
    "filtrex": "^2.2.0",
    "focus-trap-react": "^8.8.1",
    "react-datepicker": "^4.3.0",
    "react-tooltip": "^4.2.21",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.1",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^3.1.0",
    "ctix": "^1.8.2",
    "jest": "26.6.0",
    "jest-junit": "^12.0.0",
    "jest-styled-components": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.2.1",
    "type-fest": "^1.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2"
  }
