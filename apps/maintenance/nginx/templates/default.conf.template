server {
  listen 8080;

  location / {
    root /usr/share/nginx/html;
    index index.html index.htm;
    try_files $uri $uri/ /index.html =404;
  }

  location /intyg {
      root /usr/share/nginx/html;
      intyg intyg.html intyg.htm;
      try_files $uri $uri/ /intyg.html =404;
    }

  include /etc/nginx/extra-conf.d/*.conf;
}
